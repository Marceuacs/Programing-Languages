interface Two_D_Shape {
    float PI = 3.14f;

    void displayInfo();

    float calculateArea();

    float calculatePerimeter();
}

interface Three_D_Shape {
    float calculateVolume();
}

class Circle implements Two_D_Shape {
    private float radius;

    public Circle(float radius) {
        this.radius = radius;
        System.out.println("A new circle has been created");
    }

    @Override
    public void displayInfo() {
        System.out.println("Circle parameters: radius=" + radius);
    }

    @Override
    public float calculateArea() {
        return PI * radius * radius;
    }

    @Override
    public float calculatePerimeter() {
        return 2 * PI * radius;
    }
}

class Cube implements Two_D_Shape, Three_D_Shape {
    private float side;

    public Cube(float side) {
        this.side = side;
        System.out.println("A new cube has been created");
    }

    @Override
    public void displayInfo() {
        System.out.println("Cube parameters: side=" + side);
    }

    @Override
    public float calculateArea() {
        return 6 * side * side;
    }

    @Override
    public float calculatePerimeter() {
        return 0; // Perimeter is not applicable for a cube
    }

    @Override
    public float calculateVolume() {
        return side * side * side;
    }
}

public class ModifiedShapes {
    public static void main(String[] args) {
        Circle circle1 = new Circle(5);
        Cube cube1 = new Cube(4);
        Two_D_Shape circle2 = new Circle(8);
        Two_D_Shape cube2 = new Cube(3);
        Three_D_Shape cube3 = new Cube(7);

        circle1.displayInfo();
        System.out.println("Circle Area: " + circle1.calculateArea());
        System.out.println("Circle Perimeter: " + circle1.calculatePerimeter());

        cube1.displayInfo();
        System.out.println("Cube Area: " + cube1.calculateArea());
        System.out.println("Cube Volume: " + cube1.calculateVolume());

        circle2.displayInfo();
        System.out.println("Two_D_Shape Area: " + circle2.calculateArea());
        System.out.println("Two_D_Shape Perimeter: " + circle2.calculatePerimeter());

        cube2.displayInfo();
        System.out.println("Two_D_Shape Area: " + cube2.calculateArea());
        System.out.println("Two_D_Shape Perimeter: " + cube2.calculatePerimeter());

        cube3.calculateVolume(); // Since Three_D_Shape has only one method
    }
}
